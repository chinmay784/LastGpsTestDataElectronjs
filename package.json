{
  "name": "electron_js",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev:electron": "npx electronmon --trace-warnings .",
    "dev:react": "npm --prefix Electron_Js run dev",
    "dev": "concurrently \"npm run dev:electron\" \"npm run dev:react\"",
    "build:react": "npm --prefix Electron_Js run build",
    "start:prod": "npm run build:react && electron ."
  },
  "dependencies": {
    "drivelist": "^12.0.2",
    "electron": "^37.3.1",
    "lodash": "^4.17.21",
    "serialport": "^13.0.0",
    "usb": "^2.16.0",
    "usb-detection": "^4.14.2"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9"
  }
}
